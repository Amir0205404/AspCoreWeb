@model IEnumerable<WebApplication3.Models.Product>
@{
    ViewBag.Title = "Продукты";  
}

@foreach (var item in Model)
{
<div>
    
    <h5><a asp-action="Detail" asp-route-id="@item.Id"> @item.Title </a></h5> 
    <p>Цена:<b>@Model.Price</b> </p>
    <p>Описание: @item.Description</p> 

</div>
   
}