@model WebApplication3.ViewModels.HomeViewModel;
@{
    ViewData["Title"] = "Главная";
}

<div class="logo">
    <img src="~/img/config-pk.png" class="logotip"/>
</div>
<div class="catalog">
        <div class="inner-catalog">
            <div class="row">
                <div class="catalog-1">
                    <div class="inner-catalog-1">
                        @foreach (var item in Model.CategoreList)
                        {
                            <h3 class="zagalovok-catalog"> @item </h3>      
                        }
                    </div>
                </div>
                <script src="~/js/site.js">
                        
                </script>
                <div class="catalog-2">
                    <div class="inner-catalog-2">  
                        Catalog-2
                    </div>
                </div>
            </div>
        </div>
</div>
